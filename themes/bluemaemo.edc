#define BUTTON_BOTTOM_HEIGHT 0.12;
#define SWALLOWCLIP()                                                   \
    part {                                                              \
        name: "swallow_clip";                                           \
        mouse_events: 0;                                                \
        type: RECT;                                                     \
        description {                                                   \
            state: "default" 0.0;                                       \
            visible: 1;                                                 \
            rel1 {                                                      \
                relative: 0.0 0.0;                                      \
                offset: 0 0;                                            \
            }                                                           \
            rel2 {                                                      \
                relative: 1.0 1.0;                                      \
                offset: -1 -1;                                          \
            }                                                           \
            color: 255 255 255 255;                                     \
        }                                                               \
        description {                                                   \
            state: "invisible" 0.0;                                     \
            inherit: "default" 0.0;                                     \
            visible: 0;                                                 \
            color: 255 255 255 0;                                       \
        }                                                               \
    }                                                                   \
									\
     part {                                                             \
        name: "swallow_main";                                           \
        type: RECT;                                                     \
        mouse_events: 0;                                                \
        description {                                                   \
            state: "default" 0.0;                                       \
            rel1 {                                                      \
                relative: 0.0 0.0;                                      \
                offset: 0 0;                                            \
            }                                                           \
            rel2 {                                                      \
                relative: 1.0 1.0-BUTTON_BOTTOM_HEIGHT;                 \
                offset: -1 -1;                                          \
            }                                                           \
            color: 255 255 255 0;                                       \
        }                                                               \
    }									

#define CONF_TEXT(part_name,y_pos)					\
									\
    part {								\
                name: part_name;					\
                type: TEXT;						\
		effect: OUTLINE_SOFT_SHADOW;				\
                mouse_events: 0;					\
                description {						\					
                    state: "default" 0.0;				\
                    rel1 {						\
                        relative: 0.02 0.12+(y_pos*0.15);		\
                        offset: 0 0;					\
                        						\
                    }							\
                    rel2 {						\
                        relative: 0.7 0.2+(y_pos*0.15);			\
                        offset: -1 -1;					\
                    }							\
                    text {						\
                        font: "Sans";					\
			text: part_name;				\
                        size: 32;					\
                        align: 0.0 0.0;					\
                    }							\	
	       }							\
		description {                                           \
            		state: "invisible" 0.0;                         \
            		inherit: "default" 0.0;                         \
            		visible: 0;                                     \
        	}                                                       \
	}								\									

#define CONF_KEY_ICON(part_name, ypos)                         	 	 \
            part {                                                       \
                name: part_name;                                	 \
                mouse_events: 1;                                         \
                type: IMAGE;                                             \
                clip_to: "swallow_clip";                                 \
                description {                                            \
                    state: "default" 0.0;                                \
                    rel1 {                                               \
                        relative: 0.71 0.09+(ypos*0.15);	         \
                        offset: 0 0;                                     \
                        to: "swallow_clip";                              \
                    }                                                    \
                    rel2 {                                               \
                        relative: 0.99 0.22+(ypos*0.15);    		 \
                        offset: -1 -1;                                   \
                        to: "swallow_clip";                              \
                    }                                                    \
                    image {                                              \
                        normal: "button_background.png";                 \
                        border: 27 27 27 27;                             \
                    }                                                    \
                }                                                        \
                description {                                            \
                    state: "clicked" 0.0;                                \
                    inherit: "default" 0.0;                              \
                    image {normal: "button_background_active.png";}      \
			 fill { smooth: 0; }                             \
                }                                                        \
		description {                                            \
            		state: "invisible" 0.0;                          \
            		inherit: "default" 0.0;                          \
            		visible: 0;                                      \
        	}                                                        \
            }    							 \
	                                                      		 \
            part {                                                       \
                name: part_name"_icon";                           	 \
                mouse_events: 0; 					 \
		effect: OUTLINE;                                         \
                type: TEXT;                                              \
                clip_to: "swallow_clip";                                 \
                description {                                            \
                    state: "default" 0.0;                                \
                    aspect: 1.0 1.0;                                     \
                    aspect_preference: BOTH;				 \
		    color: 0 0 0 255;                                    \
                    color2: 100 100 100 255;                             \
                    color3: 100 100 100 255;                             \                             
                    rel1 {                                               \
                        relative: 0.0 0.0;                               \
                        offset: 0 0;                                     \
                        to: part_name;                         		 \
                    }                                                    \
                    rel2 {                                               \
                        relative: 1.0 1.0;                               \
                        offset: -1 -1;                                   \
                        to: part_name;                         		 \
                    }                                                    \
                   text {						 \
                	font: "key";					 \
                	size: 1;					 \
                	fit: 1 1;					 \
			text: "Ctrl+alt+a";				 \
                	align: 0.6 0.5;					 \
                  }						         \
               }                                                         \
		description {                                            \
            		state: "invisible" 0.0;                          \
            		inherit: "default" 0.0;                          \
            		visible: 0;                                      \
        	}                                                        \
            }

